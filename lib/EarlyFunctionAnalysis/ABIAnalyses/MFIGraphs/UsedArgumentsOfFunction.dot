# This analysis works both for registers and stack slots

digraph UsedArgumentsOfFunction {
  NoOrDead;
  Unused [peripheries=2];
  Yes;

  # Lattice
  NoOrDead->Unused;
  Unused->Yes;

  # Transfer functions
  Unused->Yes [label="Read"];
  Unused->NoOrDead [label="Write"];
}
